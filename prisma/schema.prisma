generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model UsersLogin{
  id Int @id @default(autoincrement())
  name String?
  username String? @unique
  password String?
  image String?
  OAuthId String?
  email String @unique
  createdAt DateTime @default(now())
}