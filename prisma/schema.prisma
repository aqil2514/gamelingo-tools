generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model UsersLogin{
  id Int @id @default(autoincrement())
  email String @unique
  username String @unique
  password String
}